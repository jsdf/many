{
  "name": "many",
  "version": "1.0.1",
  "description": "Use git worktrees with AI tools",
  "type": "module",
  "bin": {
    "many": "./dist-cli/cli/index.js"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:cli": "tsc -p tsconfig.cli.json",
    "dev": "vite build && node dist-cli/web/server.js",
    "start": "npm run build && npm run build:cli && node dist-cli/web/server.js",
    "cli": "npm run build:cli && node dist-cli/cli/index.js"
  },
  "keywords": [
    "git",
    "worktree",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.1.1"
  },
  "dependencies": {
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "simple-git": "^3.20.0",
    "superjson": "^2.2.2",
    "ws": "^8.19.0",
    "zod": "^4.0.14"
  }
}
