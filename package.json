{
  "name": "many",
  "version": "1.0.0",
  "description": "Use git worktrees with AI tools",
  "type": "module",
  "main": "out/main/index.cjs",
  "scripts": {
    "start": "electron-vite build && electron out/main/index.cjs",
    "dev": "electron-vite dev",
    "prebuild": "electron-vite build",
    "build": "tsc --noEmit && electron-vite build",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "logs": "node view-logs.js",
    "test": "npm run build && npx playwright test",
    "test:ui": "npm run build && npx playwright test --ui"
  },
  "keywords": [
    "electron",
    "git",
    "worktree",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.54.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^4.0.0",
    "esbuild": "^0.19.12",
    "playwright": "^1.54.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^7.1.1"
  },
  "dependencies": {
    "@lydell/node-pty": "^1.1.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "electron-trpc": "^0.6.2",
    "simple-git": "^3.20.0",
    "superjson": "^2.2.2",
    "zod": "^4.0.14"
  },
  "build": {
    "appId": "com.many.app",
    "productName": "Many",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "public/many-shodan.png",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "icon": "public/many-shodan.png"
    },
    "linux": {
      "icon": "public/many-shodan.png",
      "category": "Development"
    }
  }
}
