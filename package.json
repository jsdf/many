{
  "name": "many",
  "version": "1.0.0",
  "description": "Use git worktrees with AI tools",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "git",
    "worktree",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "electron-vite": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "simple-git": "^3.20.0"
  },
  "build": {
    "appId": "com.many.app",
    "productName": "Many",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "public/many-shodan.png",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "icon": "public/many-shodan.png"
